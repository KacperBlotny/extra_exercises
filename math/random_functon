#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

double tab[10000];

void fb(int x);

int f(int x);

int main()
{
    int a;
    srand(time(NULL));

    for(int i=0; i<5; i++)
    {
        a=rand()%31;
        cout << a << "\t" << f(a) <<endl;
    }
    
  return 0;
}

int f(int x)
{
    
    return (x*x)-(5*x)+6;
}
